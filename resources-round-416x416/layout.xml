<resources>

	<jsonData id="ravelOptions">{"displayMeterIcons":true}</jsonData>

	<font id="TimeFont" filename="../resources/fonts/ArialNarrow-216-Bold.fnt" antialias="true"/>
	<font id="TimeSmallFont" filename="../resources/fonts/ArialNarrow-204-Bold.fnt" antialias="true"/>

	<font id="TimeFontOutline" filename="../resources/fonts/ArialNarrow-216-Bold-Outline.fnt" antialias="true"/>
	<font id="TimeSmallFontOutline" filename="../resources/fonts/ArialNarrow-204-Bold-Outline.fnt" antialias="true"/>

	<font id="IconsFont" filename="../resources/fonts/connect32.fnt" antialias="true"/>

	<layout id="WatchFace">
		<drawable class="Background" />


		<drawable id="LeftGoalMeter" class="GoalMeter">
			<param name="side">:left</param>
			<param name="stroke">10</param>
			<param name="height">270</param>
			<param name="separator">3</param>
		</drawable>

		<drawable id="RightGoalMeter" class="GoalMeter">
			<param name="side">:right</param>
			<param name="stroke">10</param>
			<param name="height">270</param>
			<param name="separator">3</param>
		</drawable>

		<drawable id="LeftGauge" class="Gauge">
			<param name="locX">102</param>
			<param name="stroke">8</param>
			<param name="radius">28</param>
		</drawable>

		<drawable id="RightGauge" class="Gauge">
			<param name="locX">314</param>
			<param name="stroke">8</param>
			<param name="radius">28</param>
		</drawable>

		<drawable id="Time" class="ThickThinTime">
			<param name="adjustY">-22</param> <!-- y offset from screen center -->

			<!-- seconds positioning -->
			<param name="secondsX">280</param>
			<param name="secondsY">298</param>
			<param name="secondsClipWidth">52</param>
			<param name="secondsClipHeight">64</param>

			<!-- other fields positioning -->
			<param name="topFieldY">72</param>
			<param name="leftFieldAdjustX">20</param>

		</drawable>

	</layout>

</resources>