			<listEntry value="8">@Strings.StressLevel</listEntry>
			<listEntry value="9">@Strings.Respiration</listEntry>
			<listEntry value="10">@Strings.PulseOX</listEntry>
			<listEntry value="11">@Strings.Calories</listEntry>
			<listEntry value="12">@Strings.RecoveryTime</listEntry>
			<listEntry value="21">@Strings.Date</listEntry>
			<listEntry value="99">Debug</listEntry>
