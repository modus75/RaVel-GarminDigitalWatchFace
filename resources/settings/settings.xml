<settings>

	<setting propertyKey="@Properties.Theme" title="@Strings.ThemeTitle">
		<settingConfig type="list">
			<listEntry value="0">@Strings.Dark</listEntry>
			<listEntry value="1">@Strings.Light</listEntry>
			<listEntry value="2">dim 80</listEntry>
			<listEntry value="3">dim 70</listEntry>
			<listEntry value="4">dim 60</listEntry>
			<listEntry value="5">dim 50</listEntry>
			<listEntry value="6">dim 40</listEntry>
		</settingConfig>
	</setting>

	<setting propertyKey="@Properties.HoursFontType" title="@Strings.HoursFontTypeTitle">
		<settingConfig type="boolean" />
	</setting>

	<setting propertyKey="@Properties.MinutesFontType" title="@Strings.MinutesFontTypeTitle">
		<settingConfig type="boolean" />
	</setting>

	<setting propertyKey="@Properties.SecondsDisplayMode" title="@Strings.SecondsDisplayTitle">
		<settingConfig type="list">
			<listEntry value="0">@Strings.Off</listEntry>
			<listEntry value="1">@Strings.Always</listEntry>
			<listEntry value="2">@Strings.SecondsDisplayAuto</listEntry>
		</settingConfig>
	</setting>

