<resources>	

	<jsonData id="ravelOptions">{"displayMeterIcons":true}</jsonData>
	
	<font id="TimeFont" filename="../resources/fonts/ArialNarrow-126-Bold.fnt" antialias="true"/>
	<font id="TimeSmallFont" filename="../resources/fonts/ArialNarrow-102-Bold.fnt" antialias="true"/>
	
	<font id="IconsFont" filename="../resources/fonts/connect20.fnt" antialias="true"/>

	<layout id="WatchFace">
		<drawable class="Background" />
		
	
		<drawable id="LeftGoalMeter" class="GoalMeter">
			<param name="side">:left</param>
			<param name="stroke">8</param>
			<param name="height">180</param>
			<param name="separator">2</param>
		</drawable>

		<drawable id="RightGoalMeter" class="GoalMeter">
			<param name="side">:right</param>
			<param name="stroke">8</param>
			<param name="height">180</param>
			<param name="separator">2</param>
		</drawable>

		<drawable id="Time" class="ThickThinTime">
			<param name="adjustY">-14</param>
			<param name="secondsX">174</param>
			<param name="secondsY">185</param>
			<param name="secondsClipWidth">52</param>
			<param name="secondsClipHeight">38</param>

			<param name="topFieldY">41</param>
			<param name="leftFieldAdjustX">8</param>

		</drawable>

	</layout>

</resources>